"""BUILD file for vendored ssh_config crate."""

load("@rules_rust//rust:defs.bzl", "rust_library")

package(default_visibility = ["//visibility:public"])

exports_files(["Cargo.toml"])

rust_library(
    name = "ssh_config",
    srcs = glob(["src/**/*.rs"]),
    crate_name = "ssh_config",
    edition = "2021",
    rustc_flags = [
        "-Awarnings",  # Disable warnings for vendored code
    ],
)
